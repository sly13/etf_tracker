# RevenueCat API Keys
REVENUECAT_IOS_API_KEY=
# REVENUECAT_ANDROID_API_KEY=goog_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Backend Configuration
# Управление выбором бэкенда через USE_PRODUCTION_BACKEND:
# USE_PRODUCTION_BACKEND=true  - использовать продакшн URL из BACKEND_API_URL
# USE_PRODUCTION_BACKEND=false - использовать локальный URL
# Если не указано: в debug режиме используется локальный, в release - продакшн
USE_PRODUCTION_BACKEND=false
BACKEND_API_URL=https://api-etf.vadimsemenko.ru

# Локальный бэкенд (используется когда USE_PRODUCTION_BACKEND=false)
# На iOS симуляторе по умолчанию используется http://192.168.10.244:3066
# На других платформах используется http://127.0.0.1:3066
# Можно переопределить через LOCAL_BACKEND_HOST (например, LOCAL_BACKEND_HOST=192.168.1.100)
# LOCAL_BACKEND_HOST=192.168.10.244  # Раскомментируйте и укажите IP вашего Mac для iOS симулятора
API_TIMEOUT=30000

# App Configuration
APP_NAME=ETF Flow
APP_VERSION=1.0.0

# Legal Documents URLs
TERMS_OF_USE_URL=https://www.apple.com/legal/internet-services/itunes/dev/stdeula/
PRIVACY_POLICY_URL=https://www.privacypolicies.com/live/2e0d5b0f-786f-45a6-be4e-e7bc311d30d6
